<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <link rel="stylesheet" href="../styles/style.css">
    </head>

    <body>
        <header>
            <div>
                <p>News</p>
            </div>
        </header>
        <main>
            <% data.forEach((post)=>{ %>
                <article>
                    <div>
                        <img src=" <%= post.image_url %>" alt="">
                    </div>
                    <div>
                        <h1>
                            <%= post.title %>
                        </h1>
                        <p>
                            <%= post.description %>
                        </p>
                    </div>
                </article>
                <% }) %>
        </main>
    </body>

</html>